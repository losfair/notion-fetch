{
    "name": "notion-fetch",
    "version": "1.0.0",
    "description": "Fetch Notion pages as Markdown.",
    "main": "dist/worker.js",
    "scripts": {
        "build": "webpack; gsed -i 's/__webpack_exports___esModule as __esModule, //' dist/worker.mjs",
        "format": "prettier --write  '*.{json,js}' 'src/**/*.{js,ts}' 'test/**/*.{js,ts}'",
        "lint": "eslint --max-warnings=0 src && prettier --check '*.{json,js}' 'src/**/*.{js,ts}' 'test/**/*.{js,ts}'"
    },
    "author": "Heyang Zhou <zhy20000919@hotmail.com>",
    "license": "Apache-2.0",
    "eslintConfig": {
        "root": true,
        "extends": [
            "typescript",
            "prettier"
        ]
    },
    "devDependencies": {
        "@cloudflare/workers-types": "^3.0.0",
        "@types/react": "^18.0.15",
        "@types/react-dom": "^18.0.6",
        "@types/service-worker-mock": "^2.0.1",
        "@types/showdown": "^2.0.0",
        "@types/turndown": "^5.0.1",
        "@typescript-eslint/eslint-plugin": "^4.16.1",
        "@typescript-eslint/parser": "^4.16.1",
        "eslint": "^7.21.0",
        "eslint-config-prettier": "^8.1.0",
        "eslint-config-typescript": "^3.0.0",
        "notion-types": "^6.12.6",
        "prettier": "^2.3.0",
        "ts-loader": "^9.2.2",
        "typescript": "^4.3.2",
        "webpack": "^5.38.1",
        "webpack-cli": "^4.7.0"
    },
    "dependencies": {
        "@tsndr/cloudflare-worker-jwt": "^1.1.5",
        "blakejs": "^1.2.1",
        "notion-utils": "^6.12.9",
        "p-map": "^5.5.0",
        "react": "^16.14.0",
        "react-dom": "^16.14.0",
        "react-notion": "^0.10.0",
        "react-notion-x": "^6.12.11",
        "showdown": "^2.1.0",
        "turndown": "^7.1.1"
    }
}
